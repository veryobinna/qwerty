{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "plugin:react/recommended",
      "airbnb-typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaFeatures": {
          "tsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module",
      "project": ["./tsconfig.json"],
      "extraFileExtensions": [".css"]
  },
  "plugins": [
      "react",
      "import",
      "jsx-a11y",
      "@typescript-eslint"
  ],
  "rules": {
      "no-underscore-dangle": "off",
      "@typescript-eslint/comma-dangle":"off"
      
  }
}
